    "webpack": "^4.31.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "aws-sdk": "^2.814.0",
    "babel-runtime": "^6.26.0",
